{
  "name": "log-debug",
  "version": "1.1.5",
  "description": "Provide logger written in TypeScript for TypeScript or JavaScript.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "preinstall": "typings install",
    "postinstall": "tsc",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/romakita/log-debug"
  },
  "keywords": [
    "log",
    "debug",
    "typescript"
  ],
  "author": "Romakita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romakita/log-debug/issues"
  },
  "homepage": "https://github.com/romakita/log-debug",
  "devDependencies": {
    "chai": "^2.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "colors": "^1.1.0",
    "source-map-support": "^0.4.0"
  }
}
